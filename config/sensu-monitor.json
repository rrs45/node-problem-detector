{
	"plugin": "sensulog",
	"pluginConfig": {
		"checks": "check_docker_device_mapper",
		"message": "Payload.Check.Name"
		"timestampFormat": "TimeStamp"
	},
	},
	"logPath": "/var/log/sensu/sensu.log",
	"lookback": "5m",
	"bufferSize": 10,
	"source": "sensu-monitor",
	"conditions": [
    		{
      			"type": "SensuChecks",
     			"reason": "NoFailures",
      			"message": "All checks passed"
		}
  	],
	"rules": [
		{
			"type": "permanent",
      			"condition": "SensuChecks",
			"reason": "ChecksFailed",
			"pattern": ["critical", "warn"]
		}
	]
}
